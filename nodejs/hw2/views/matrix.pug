doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible" content="IE=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        title Matrix
        style 
            include ../public/style.css 
    body
        #container
            each row in matrix
                p#row
                    each val in row 
                        span= val